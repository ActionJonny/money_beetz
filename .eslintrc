{
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "quotes": [0],
    "eol-last": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "no-unused-vars": [0],
    "arrow-body-style": [0],
    "no-unused-expressions": [0],
    "no-shadow": [0],
    "dot-notation": [0],
    "prefer-arrow-callback": [0],
    "comma-dangle": [0],
    "consistent-return": [0],
    "func-names": [0],
    "no-else-return": [0],
    "no-throw-literal": [0],
    "import/no-unresolved": [0],
    "react/jsx-filename-extension": [0],
    "react/prefer-stateless-function": [0],
    "max-len": [0],
    "comma-spacing": [0],
    "no-useless-constructor": [0],
    "no-undef": [0],
    "class-methods-use-this": [0],
    "import-extensions": [0],
    "arrow-parens": [0],
    "no-plusplus": [0],
    "import/first": [0],
    "import/extensions": [0],
    "no-console": [0],
    "no-param-reassign": [0],
    "padded-blocks": [0],
    "no-useless-computed-key": [0],
    "eqeqeq": [0],
    "no-path-concat": [0],
    "prefer-template": [0]
  }
}
